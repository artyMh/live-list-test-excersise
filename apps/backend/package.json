{
  "name": "@app/backend",
  "version": "1.0.0",
  "description": "Application's backend app",
  "author": "https://github.com/artyMh",
  "private": true,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyMh/live-list-test-excersise.git"
  },
  "type": "module",
  "scripts": {
    "start": "node dist/index.mjs",
    "start:dev": "tsx watch ./src/index.mts",
    "lint": "eslint \"src/**/*.ts\" --max-warnings 5 --report-unused-disable-directives",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "prebuild": "pnpm run clean",
    "build": "tsc --build tsconfig.build.json && resolve-tspaths",
    "clean": "tsc --build tsconfig.build.json --clean",
    "postclean": "rimraf dist"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "10.15.0"
  },
  "packageManager": "pnpm@10.15.0",
  "dependencies": {
    "@app/core": "workspace:*",
    "@app/eslint-config": "workspace:*",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "frameguard": "^4.0.0",
    "socket.io": "^4.7.5",
    "uuid": "^11.0.5",
    "winston": "^3.13.1",
    "x-xss-protection": "^2.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "eslint": "^9.35.0",
    "resolve-tspaths": "^0.8.23",
    "rimraf": "^6.0.1",
    "tsx": "^4.16.2",
    "typescript": "^5.7.3"
  }
}
