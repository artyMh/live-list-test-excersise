{
  "name": "@app/root",
  "version": "1.0.0",
  "description": "Test excersise",
  "author": "https://github.com/artyMh",
  "private": true,
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artyMh/live-list-test-excersise.git"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "tsc --build --clean",
    "build": "tsc --build",
    "build:frontend": "pnpm run --filter \"@app/frontend\" build",
    "build:backend": "pnpm run --filter \"@app/backend\" build",
    "start:frontend": "pnpm run --filter \"@app/frontend\" start:dev",
    "start:backend": "pnpm run --filter \"@app/backend\" start:dev"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": ">=22.0.0",
    "pnpm": "10.15.0"
  },
  "packageManager": "pnpm@10.15.0",
  "devDependencies": {
    "typescript": "^5.7.3"
  }
}
